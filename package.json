    "@trpc/client": "11.0.0-rc.781",
    "@trpc/server": "11.0.0-rc.781",
    "@vscode/l10n": "^0.0.18",
    "allotment": "^1.20.3",
    "antlr4ts": "^0.5.0-alpha.4",
    "bson": "^6.10.3",
