    "terser-webpack-plugin": "^5.3.13",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "webpack": "~5.95.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "@azure/arm-compute": "^22.4.0",
    "@azure/arm-cosmosdb": "16.0.0-beta.7",
