    "semver": "^7.7.1",
    "slickgrid-react": "~5.8.0",
    "uuid": "^11.1.0",
    "vscode-json-languageservice": "^5.4.3",
    "vscode-languageclient": "~6.1.4",
    "vscode-languageserver": "~6.1.1",
